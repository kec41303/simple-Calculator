    private void btnCalculateActionPerformed(java.awt.event.ActionEvent evt) {                                             
String answer;

Secondnumber = Double.parseDouble(lblDisplay.getText());

if (aperation == "+")
{
result = Firstnumber + Secondnumber ;
answer = String.format("%.of", result);
  lblDisplay.setText(answer);
}
else if (aperation == "-")
{
result = Firstnumber - Secondnumber ;
answer = String.format("%.of", result);
  lblDisplay.setText(answer);
}

else if (aperation == "x")
{
result = Firstnumber * Secondnumber ;
answer = String.format("%.of", result);
  lblDisplay.setText(answer);
}
else if (aperation == "/")
{
result = Firstnumber / Secondnumber ;
answer = String.format("%.of", result);
  lblDisplay.setText(answer);
}
else if (aperation == "%")
{
result = Firstnumber % Secondnumber ;
answer = String.format("%.of", result);
  lblDisplay.setText(answer);
}



    }                                            

